	rno=raw_input()
	print "enter the branch"
	br=raw_input()
	usn="4pa"+year+br+rno
	payload={'rid':usn,'submit':'submit'}
	r=requests.post("http://results.vtu.ac.in/vitavi.php/post",data=payload)


def main():
	ret()
	parsehtml()
	getval()
	
	return 0

if __name__ == '__main__':
